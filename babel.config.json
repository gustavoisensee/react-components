{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.4"
      }
    ],
    "@babel/preset-react"
  ],
  "ignore": [
    "__tests__",
    "__mock__",
    "__snapshots__",
    "src/**/*.stories.js",
    "src/**/*.test.js",
    "src/**/*.mock.js"
  ]
}